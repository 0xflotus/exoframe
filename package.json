{
  "name": "exoframe",
  "version": "0.5.1",
  "description": "Exoframe - power armor for docker containers",
  "main": "index.js",
  "bin": {
    "exoframe": "bin/index.js"
  },
  "scripts": {
    "start": "nodemon --debug --ignore dist/ --exec 'npm run build'",
    "build": "rm -rf dist/ && babel src -d dist",
    "prepublish": "npm run build",
    "test": "NODE_ENV=testing node test/index.js | tap-spec",
    "cover": "NODE_ENV=testing babel-node ./node_modules/istanbul/lib/cli cover ./node_modules/tape/bin/tape ./test",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "docker",
    "cli",
    "exoframe",
    "build",
    "deploy"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:exoframejs/exoframe.git"
  },
  "author": "Tim Ermilov <yamalight@gmail.com> (http://codezen.net)",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015-node",
      "stage-0"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.2",
    "coveralls": "^2.11.13",
    "express": "^4.14.0",
    "istanbul": "^1.1.0-alpha.1",
    "nodemon": "^1.10.2",
    "sinon": "^1.17.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "char-spinner": "^1.0.1",
    "got": "^6.3.0",
    "inquirer": "^1.1.2",
    "js-yaml": "^3.6.1",
    "minimatch": "^3.0.3",
    "tar-fs": "^1.13.2",
    "yargs": "^5.0.0"
  }
}
