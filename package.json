{
  "name": "exoframe",
  "version": "3.1.0-dev",
  "description": "Exoframe is a self-hosted tool that allows simple one-command deployments using Docker",
  "main": "index.js",
  "repository": "git@github.com:exoframejs/exoframe.git",
  "author": "Tim Ermilov <yamalight@gmail.com>",
  "license": "MIT",
  "bin": {
    "exoframe": "bin/index.js"
  },
  "scripts": {
    "test": "TZ=Greenland NODE_ENV=testing jest --coverage --silent --maxWorkers=2 --ci",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "package": "pkg --targets node8.9.0-linux-x64,node8.9.0-win-x64,node8.9.0-macos-x64 -o exoframe index.js"
  },
  "dependencies": {
    "boxen": "^1.2.2",
    "chalk": "^2.4.1",
    "cli-table": "^0.3.1",
    "got": "^8.3.2",
    "highland": "^2.13.0",
    "ignore": "^4.0.2",
    "inquirer": "^6.0.0",
    "js-yaml": "^3.12.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "opn": "^5.3.0",
    "ora": "^2.1.0",
    "tar-fs": "^1.16.3",
    "update-notifier": "^2.5.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "jest": "^23.3.0",
    "nock": "^9.4.1",
    "pkg": "^4.3.3",
    "sinon": "^6.1.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
